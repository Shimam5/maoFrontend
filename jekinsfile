pipeline {
    agent any
   tools { 
        nodejs "node"
    }
    stages{
        stage('Git '){
            steps{
               git 'https://github.com/Shimam5/maoFrontend.git'
            }
        }
        stage('Install'){
            steps{
                sh 'npm install -g yarn'
                 sh   'brew install gcc'
                   sh 'brew install yarn'
                    sh 'brew install node'

            
            }
        }
        stage('build'){
            stage{
                sh ' echo done '
            }
        }
        stage('Test'){
            steps{
                sh 'node test'
                
            }
        }
    }
   
}

